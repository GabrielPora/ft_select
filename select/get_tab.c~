t_select get_line_colum(int argc, char **argv)
{
  t_select sl;
  struct winsize ws;

  if ((sl.max_col = get_column(argc, &ws)) == 0)
    {
      sl.lc = NULL;
      return (sl);
    }
  sl.max_row = ws.ws_row;
  sl.col_len = ws.ws_col;
  list.argc = argc;
  sl.tb = get_tab(argc, argv, list.max_row, list.max_col);
  get_space_number(&sl);
  check_window_size(list);
}
